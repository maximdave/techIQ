                                                <tr>
                                                    <th>S/N</th>
                                                    <th>Student ID</th>
                                                    <th>Fullname</th>
                                                    <th>Email</th>
                                                    <th>Address</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for (var each = 0; each < user.length; each++){ %>
                                                    <% if(user[each].status == 'Active' && user[each].confirm =='Confirmed'){%>
                                                    <tr>
                                                        <td><%= each + 1  %></td>
                                                        <td><%= user[each]._id  %></td>
                                                        <td><%= user[each].name  %></td>
                                                        <td><%= user[each].email  %></td>
                                                        <td><%= user[each].address  %></td>
                                                        <td> <a href="/api/block?id=<%= user[each]._id  %>"><button class="btn btn-danger">Block Student</button></a> </td>
                                                    </tr>
                                                    <%}%>
                                                <%}%>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
